<core:FragmentDefinition
xmlns="sap.m"
xmlns:core="sap.ui.core">
<Dialog contentWidth="400px" contentHeight="350px" title="Select Volume Unit">
<subHeader>
<Toolbar>
<SearchField id = "IdSearchLbhUnit"
search ="handleSrchLbhUnit">
</SearchField>
</Toolbar>
</subHeader>
<content>

<List id="ListIdLbhUnit" items="{LbhUnitModel>/}" mode="SingleSelectMaster" selectionChange="onLbhUnitListSelect">
<StandardListItem title="{LbhUnitModel>UomLwh}"
description="{LbhUnitModel>UnitText}"> <!-- description="{Name2}"> -->
</StandardListItem>
</List>
</content>
<endButton>
<Button text="Cancel" press="handleLwhF4SelectDialogclose"/>
</endButton>
</Dialog>
</core:FragmentDefinition>

